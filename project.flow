{
  snow: {
    config: {
      log: 4,
      config_path: 'assets/json/config/config.json'
    }
  },
  project: {
    name: 'luxe-template',
    version: '1.0.0',
    author: 'zollenz',
    app: {
      main: 'Main',
      name: 'luxe-template',
      package: 'is.mios.luxetemplate'
    },
    build: {
      defines: [
        'MACOSX_VER=10.12'
      ],
      flags: [
        '--macro luxe.Log.level(2)'
      ],
      dependencies: {
        luxe: '*',
        mint: '*'
      }
    },
    files: {
      assets: 'assets/'
    }
  }
}