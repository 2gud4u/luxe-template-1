{
    "displayName": "▶︎ Mac application",
    "description": "haxelib run flow run mac",
    "command": "haxelib",
    "args": [
        "run",
        "flow",
        "run",
        "mac",
        "--debug"
    ],
    "version": "0.1.0",
    "problemMatcher": {
        "owner": "haxe",
        "pattern": {
            "regexp": "^(.+):(\\d+): (?:lines \\d+-(\\d+)|character(?:s (\\d+)-| )(\\d+)) : (?:(Warning) : )?(.*)$",
            "file": 1,
            "line": 2,
            "endLine": 3,
            "column": 4,
            "endColumn": 5,
            "severity": 6,
            "message": 7
        }
    },
    "chooserIndex": 0
}